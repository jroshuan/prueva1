#include <iostream>

using namespace std;

int main() {
    int num1, num2, suma;

    // Solicitar al usuario que ingrese dos números
    cout << "Ingresa el primer número: ";
    cin >> num1;

    cout << "Ingresa el segundo número: ";
    cin >> num2;

    // Realizar la suma
    suma = num1 + num2;

    // Mostrar el resultado
    cout << "La suma de " << num1 << " y " << num2 << " es: " << suma << endl;

    return 0;
}
